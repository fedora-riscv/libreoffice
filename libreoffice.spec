%{baseinstdir}/program/resource/*%{1}.res  \
%{baseinstdir}/share/config/soffice.cfg/modules/*/ui/res/%{1}.zip \
%{baseinstdir}/share/config/soffice.cfg/*/ui/res/%{1}.zip \
